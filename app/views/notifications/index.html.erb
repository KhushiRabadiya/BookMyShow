<h2>Notification Index</h2>
<table class="t1">
  <thead>
    <th>Notifiable type</th>
    <th>Name</th>
    <th>Massage</th>
  </thead>
  <tbody>
    <% @notifications.each do |notification| %>
      <tr>
        <td><%= notification.notifiable_type %></td>
        <td><%= notification.display_name %></td>
        <td><%= notification.message %></td>
      </tr>
    <% end %>
  </tbody>
</table>
